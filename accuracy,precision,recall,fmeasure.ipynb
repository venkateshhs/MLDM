    "tp = 2\n",
    "fp = 3\n",
    "tn = 2\n",
    "fn = 3\n",
    "total = 10\n",
    "#accuracy\n",
    "acc = (tp + tn)/total\n",
    "prec = tp/(tp + fp)\n",
    "recall = tp/(tp + fn)\n",
    "fmeas = (2 * prec * recall)/(prec + recall)\n",
    "print(\"Accuracy:\",acc)\n",
    "print(\"Precision:\",prec)\n",
    "print(\"Recall:\",recall)\n",
    "print(\"F-measure:\",fmeas)"
